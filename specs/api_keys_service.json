        "description": "This data structure is the response obtained when creating an API key.",
        "type": "object",
        "properties": {
          "keyId": {
            "example": "my_key_id",
            "type": "string"
          },
          "name": {
            "example": "my_api_key",
            "type": "string"
          },
          "value": {
