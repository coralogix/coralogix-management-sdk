          "url": ""
        },
        "operationId": "ViewsService_GetView",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "example": 52,
              "description": "id",
              "type": "integer",
              "format": "int32"
            },
            "style": "simple"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/View"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "Views service"
        ],
        "summary": "Replace a view service",
        "description": "Replaces an existing view",
        "externalDocs": {
          "url": ""
        },
        "operationId": "ViewsService_ReplaceView",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "example": 52,
              "description": "id",
              "type": "integer",
              "format": "int32"
            },
            "style": "simple"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "View",
                "description": "Response for views.",
                "type": "object",
                "properties": {
                  "filters": {
                    "$ref": "#/components/schemas/SelectedFilters"
                  },
                  "folderId": {
                    "example": "3dc02998-0b50-4ea8-b68a-4779d716fa1f",
