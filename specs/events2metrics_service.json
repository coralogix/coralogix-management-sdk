          },
          "type": {
            "$ref": "#/components/schemas/E2MType"
          },
          "updateTime": {
            "example": "2022-06-30T12:30:00Z'",
            "type": "string"
          }
        },
        "required": [
          "name",
          "type"
        ],
        "additionalProperties": false
