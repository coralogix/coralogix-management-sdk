                "definition": {
                  "lineChart": {
                    "queryDefinitions": [
                      {
                        "id": "id",
                        "seriesNameTemplate": "[{{ subsystem }}]: {{ kubernetes.pod_id.keyword }}",
                        "query": {
                          "logs": {
                            "luceneQuery": {
                              "value": "coralogix.metadata.severity=5 OR coralogix.metadata.severity=\"6\" OR coralogix.metadata.severity=\"4\""
                            },
                            "groupBy": [
                              "coralogix.metadata.subsystemName"
                            ],
                            "aggregations": [
                              {
                                "count": {}
                              }
                            ]
                          }
                        }
                      }
